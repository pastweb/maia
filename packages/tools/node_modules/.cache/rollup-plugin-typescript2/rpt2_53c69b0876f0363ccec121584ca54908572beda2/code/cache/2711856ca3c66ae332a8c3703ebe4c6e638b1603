{"code":"import { __assign } from \"tslib\";\r\nimport { isSSR } from '../isSSR';\r\nexport var uaRegex = /iP(hone|ad|od)|blackberry|Android|(W|w)indows (P|p)hone/g;\r\nfunction checkWithMediaQuery(mql) {\r\n    var matches = mql.matches;\r\n    return uaRegex.test(window.navigator.userAgent) || matches ? true : false;\r\n}\r\nexport function isMobile(params) {\r\n    if (params === void 0) { params = {}; }\r\n    var isMobile = params.isMobile, mediaQueryString = params.mediaQueryString, onMediaQueryChange = params.onMediaQueryChange;\r\n    if (typeof isMobile !== 'undefined')\r\n        return isMobile;\r\n    if (isSSR)\r\n        return false;\r\n    if (!mediaQueryString) {\r\n        return uaRegex.test(window.navigator.userAgent);\r\n    }\r\n    function _onMediaQueryChange(event) {\r\n        var isMobile = checkWithMediaQuery(event);\r\n        if (onMediaQueryChange) {\r\n            onMediaQueryChange(__assign(__assign({}, event), { isMobile: isMobile }));\r\n        }\r\n    }\r\n    var mql = window.matchMedia(mediaQueryString);\r\n    if (onMediaQueryChange) {\r\n        mql.addEventListener('change', _onMediaQueryChange);\r\n    }\r\n    return checkWithMediaQuery(mql);\r\n}\r\n//# sourceMappingURL=isMobile.js.map","references":["/Users/nico/Projects/maia/@maia/packages/tools/src/isSSR/index.ts","/Users/nico/Projects/maia/@maia/packages/tools/src/isMobile/types.ts"],"map":"{\"version\":3,\"file\":\"isMobile.js\",\"sourceRoot\":\"\",\"sources\":[\"../../../../../src/isMobile/isMobile.ts\"],\"names\":[],\"mappings\":\";AAAA,OAAO,EAAE,KAAK,EAAE,MAAM,UAAU,CAAC;AAGjC,MAAM,CAAC,IAAM,OAAO,GAAG,0DAA0D,CAAC;AAElF,SAAS,mBAAmB,CAAC,GAAyC;IAC5D,IAAA,OAAO,GAAK,GAAG,QAAR,CAAS;IACxB,OAAO,OAAO,CAAC,IAAI,CAAC,MAAM,CAAC,SAAS,CAAC,SAAS,CAAC,IAAI,OAAO,CAAC,CAAC,CAAC,IAAI,CAAC,CAAC,CAAC,KAAK,CAAC;AAC5E,CAAC;AAED,MAAM,UAAU,QAAQ,CAAC,MAAsB;IAAtB,uBAAA,EAAA,WAAsB;IACrC,IAAA,QAAQ,GAA2C,MAAM,SAAjD,EAAE,gBAAgB,GAAyB,MAAM,iBAA/B,EAAE,kBAAkB,GAAK,MAAM,mBAAX,CAAY;IAClE,IAAI,OAAO